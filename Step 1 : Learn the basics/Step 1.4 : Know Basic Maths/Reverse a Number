//--> LeetCode <--

class Solution {
public:
    int reverse(int x) {
        
        int ans = x;
        long long int rem = 0;

        while(ans != 0)
        {
            int digit = ans%10;
            rem = (rem * 10) + digit;
            if ((rem > INT_MAX)||(rem< INT_MIN))
            {
                return 0;
            }
            ans = ans/10;
        }
        return rem;
    }
};



//--> CodeStudio <--

#include<iostream>
using namespace std;

int main() {
	int n;
	cin >> n;

	int rem = 0;
	while(n != 0)
	{
		int digit = n%10;
		rem = rem * 10 + digit;
		n = n/10;
	}
	cout<< rem;
}

